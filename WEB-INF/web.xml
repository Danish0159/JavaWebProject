	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>Products</servlet-name>
		<url-pattern>/Products</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>Checkout</servlet-name>
